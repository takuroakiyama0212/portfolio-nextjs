{
  "version": 2,
  "buildCommand": "npm run build:web",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/_expo/static/(.*)",
      "destination": "/api/index.ts"
    },
    {
      "source": "/web/_expo/static/(.*)",
      "destination": "/api/index.ts"
    },
    {
      "source": "/web",
      "destination": "/api/index.ts"
    },
    {
      "source": "/web/(.*)",
      "destination": "/api/index.ts"
    },
    {
      "source": "/assets/(.*)",
      "destination": "/api/index.ts"
    },
    {
      "source": "/",
      "destination": "/api/index.ts"
    },
    {
      "source": "/manifest",
      "destination": "/api/index.ts"
    },
    {
      "source": "/download",
      "destination": "/api/index.ts"
    }
  ],
  "headers": [
    {
      "source": "/_expo/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/web/_expo/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}

